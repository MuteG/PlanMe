<Window xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:v="clr-namespace:PlanMe.Views"
        xmlns:vm="using:PlanMe.ViewModels"
        mc:Ignorable="d" d:DesignWidth="200" d:DesignHeight="300"
        Width="300" Height="500" WindowStartupLocation="CenterScreen"
        x:Class="PlanMe.Views.MainWindow"
        x:DataType="vm:MainWindowViewModel"
        Icon="/Assets/PlanMe.ico"
        Title="PlanMe">

    <v:InboxView DataContext="{Binding Inbox}"/>

</Window>